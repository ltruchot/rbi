#interface-box
  padding-left 0

#interface-box h1
  background-color #f1f1f1
  min-height 30px
  color #2b2b2b
  text-align center
  text-transform uppercase
  font-size 1.4em
  padding 3px 0
  margin 0

.plain-icon-blue
  color #fff
  background-color #40acd6
  padding 2px 6px 1px 6px
  font-size 14px
  line-height 0.9
  margin-right 1px

.plain-icon-red
  color #fff
  background-color #f74e4d
  padding 2px 6px 1px 6px
  font-size 14px
  line-height 0.9
  margin-right 1px

.plain-icon-blue:before, .plain-icon-red:before
  font-size 14px



.search-panel
  margin auto
.search-btn
  width 200px
  height 50px
  color #fff
  display block
  border none
  padding 5px
  cursor pointer
  position relative


.grey1
  background-color #E0E0E0

.grey2
  background-color #B0B0B0

.grey3
  background-color #808080

.grey4
  background-color #505050

.search-btn span
  display block

.search-field
  background-color #f1f1f1
  width 100%
  border 0
  border-radius 10px
  padding 0
  height 25px
  position relative

.search-field #search-text
  background-color #f1f1f1
  width 100%
  border 0
  border-radius 10px
  display block
  height 25px
  padding-left 20px
  padding-right 10px


.search-field #search-text:focus
  border 0
  outline-offset 0
  outline none

.search-field .icon-search
  display block
  float left
  margin-right 5px
  position absolute
  top 3px
  left 5px

.big-size-icon
  font-size 1.4em
  padding 2px 0 0 5px
.big-size-icon:before
  font-size 1.4em

.big-size-text
  font-size 1.2em
  font-weight bold

.little-size-text
  font-size 0.9em
  position absolute
  bottom 5px
  right 5px

.google-pie-chart svg, .google-pie-chart div
  padding-bottom 10px !important
